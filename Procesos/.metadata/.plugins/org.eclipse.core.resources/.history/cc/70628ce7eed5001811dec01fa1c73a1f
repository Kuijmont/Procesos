package ejercicios;

public class Suma {

	public static void main(String[] args) {
		
		sumar(5,10);
		
		System.out.println("La suma es: "+sumar(5,10));
		
	}

	private static int sumar(int a, int b) {
		int suma = 0;
		for (int i = a; i <= b; i++) {
			suma=suma+i;
		}
		return suma;
	}	
}

